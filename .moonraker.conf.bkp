[server]
host = 0.0.0.0
port = 7125

[file_manager]
config_path = /home/pi/klipper_config
log_path = /home/pi/klipper_logs

[database]
database_path = ~/.moonraker_database

[data_store]
temperature_store_size = 600
gcode_store_size = 1000

[authorization]
trusted_clients = 
	127.0.0.1
	192.168.0.0/16
	::1/128
	FE80::/10
cors_domains = 
	*.lan
	*.local
	*://my.mainsail.xyz
	*://app.fluidd.xyz

[octoprint_compat]

[history]

[update_manager]

[update_manager client mainsail]
type = web
repo = meteyou/mainsail
path = ~/mainsail

[update_manager client fluidd]
type = web
repo = cadriel/fluidd
path = ~/fluidd

